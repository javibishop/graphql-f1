{
  "name": "graphql-f1",
  "version": "1.0.0",
  "description": "Api graphql F1",
  "main": "build/server.js",
  "scripts": {
    "start": "node build/server.js",
    "build": "tsc -p . && ncp src/schema build/schema",
    "start:dev": "npm run build:dev",
    "build:dev": "nodemon \"src/server.ts\" --exec \"ts-node\" src/server.ts -e ts,graphql,json",
	"publish": "npm run build && now --prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/javibishop/graphql-f1.git"
  },
  "keywords": [
    "API",
    "Graphql",
    "F1"
  ],
  "author": "javier edgardo <javibishop@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/javibishop/graphql-f1/issues"
  },
  "homepage": "https://github.com/javibishop/graphql-f1#readme",
  "engines": {
    "node": "v10.15.3"
  },
  "dependencies": {
    "apollo-datasource-rest": "^0.8.0",
    "apollo-server": "^2.14.2",
    "apollo-server-express": "^2.6.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "graphql": "^14.3.1",
    "graphql-import": "^0.7.1",
    "graphql-import-node": "0.0.4",
    "graphql-playground-middleware-express": "^1.7.12",
    "graphql-tag-pluck": "^0.8.7",
    "graphql-tools": "^4.0.4",
    "http": "0.0.0",
    "ncp": "^2.0.0",
    "typescript": "^3.5.1"
  },
  "devDependencies": {
    "@types/compression": "0.0.36",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/node": "^12.0.4",
    "nodemon": "^2.0.1",
    "ts-node": "^8.5.4"
  }
}
